def vsco_code(nombre: str) -> str:
    """
    Reproduction exacte VSCO I-button :
    1. On prend le nombre sous forme de chaîne pour garder les zéros
    2. Inversement en fraction flottante
    3. On récupère les 7 derniers chiffres visibles de la calculatrice
    """
    n = int(nombre)          # conversion en int
    inv = 1 / n              # bouton I
    # transformer en chaîne sans la virgule
    s = f"{inv:.15f}"        # 15 décimales comme une calculatrice
    s = s.replace("0.", "")  # supprimer "0."
    return s[:7]             # garder les 7 premiers chiffres

# Test
valeurs = ["0429733", "0429735", "0429752"]
for v in valeurs:
    print(v, "→", vsco_code(v))
